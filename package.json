{
  "name": "FitMate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod-install": "npx pod-install -ios",
    "pod-install-M1": "cd ios && arch -x86_64 pod install && cd ..",
    "reset-cache": "killall -9 node && react-native start --reset-cache",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "ios-clean": "cd ios && xcodebuild clean && cd ..",
    "pod-update": "cd ios && arch -x86_64 pod repo update",
    "android-release": "react-native run-android --variant=release",
    "apk-release": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "aab-release": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "signingReport": "cd android && ./gradlew signingReport",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "codepush:ios": "appcenter codepush release-react -a imsgroup/SGO -d Production -m",
    "codepush:android": "appcenter codepush release-react -a imsgroup/SGO_ANDROID -d Production -m",
    "codepush-key:ios": "appcenter codepush deployment list -a imsgroup/SGO -k",
    "codepush-key:android": "appcenter codepush deployment list -a imsgroup/SGO_ANDROID -k"
  },
